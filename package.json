    "docker-build": "yarn docker-build:angularapp && yarn docker-build:nestapp && yarn docker-build:nextapp && yarn docker-build:nginx",
    "docker-run:angularapp": "docker run -p 4200:4200 angularapp",
    "docker-run:nestapp": "docker run -p 3333:3333 nestapp",
    "docker-run:nextapp": "docker run -p 3000:3000 nextapp",
